{
  "name": "headscale-web",
  "version": "0.11.4",
  "description": "Web interface for headscale 0.23.0",
  "homepage": "https://itsib.su",
  "license": "MIT",
  "author": {
    "email": "itsib.su@gmail.com",
    "name": "Sergey Kraynow"
  },
  "config": {
    "name": "Headscale Web UI",
    "short-name": "Headscale Web",
    "theme-color": "rgb(31 30 30)",
    "background-color": "rgb(31 30 30)"
  },
  "private": false,
  "type": "module",
  "browser": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsib/headscale-web.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "build": "npm run build:icons && tsc -b && NODE_ENV=production vite build --mode production && npm run build:copy",
    "build:dev": "tsc -b && NODE_ENV=development vite build --mode development && npm run build:copy",
    "build:icons": "icon-font g",
    "build:copy": "node ./scripts/copy.mjs",
    "lint": "eslint . && prettier --log-level warn --check .",
    "lint:fix": "prettier --log-level warn --write . && eslint . --fix",
    "test": "vitest",
    "preview": "vite preview",
    "preversion": "npm run lint && npm run test",
    "version": "node ./scripts/version.mjs && git add .",
    "postversion": "git push && git push --tags"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.136.11",
    "@tanstack/react-router-devtools": "^1.136.11",
    "i18next": "^25.6.2",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^16.3.3",
    "react-just-ui": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@itsib/icon-font": "^0.4.8",
    "@tanstack/router-plugin": "^1.136.11",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "postcss-nested": "^7.0.2",
    "prettier": "3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^4.0.10"
  },
  "overrides": {
    "esbuild": "^0.25.12"
  }
}
