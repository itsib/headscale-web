{
  "name": "headscale-web",
  "version": "0.2.17",
  "description": "Web interface for headscale 0.23.0",
  "homepage": "https://itsib.su",
  "license": "MIT",
  "author": {
    "email": "itsib.su@gmail.com",
    "name": "Sergey Kraynow"
  },
  "private": false,
  "type": "module",
  "browser": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsib/headscale-web.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "build": "tsc -b && NODE_ENV=production vite build --mode production",
    "build:dev": "tsc -b && NODE_ENV=development vite build --mode development",
    "build:icons": "fantasticon --config icon-font/config.cjs --normalize --round=1024 --debug",
    "jwt:secret": "node --env-file=./.env scripts/jwt-secret-jose.mjs",
    "jwt:token": "node --env-file=./.env ./scripts/jwt-token.mjs",
    "lint": "eslint .",
    "test": "vitest",
    "preview": "vite preview",
    "preversion": "npm run lint && npm run test",
    "version": "node ./scripts/version.mjs && git add .",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@codemirror/commands": "^6.7.0",
    "@codemirror/language": "^6.10.3",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.34.1",
    "@lezer/highlight": "^1.2.1",
    "@lezer/lr": "^1.4.2",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-persist-client": "^5.56.2",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^3.0.0",
    "jose": "^5.9.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.2.0",
    "react-just-ui": "^0.6.1",
    "react-router-dom": "^7.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@itsib/icon-font": "^0.3.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-visitor-keys": "^4.2.0",
    "express": "^4.21.2",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "sirv": "^3.0.0",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.2",
    "typescript": "~5.6.0",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.5",
    "vite-plugin-compression2": "^1.3.0",
    "vite-plugin-cp": "^4.0.8",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.8",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  }
}
