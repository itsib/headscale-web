{
  "name": "headscale-web",
  "version": "0.1.3",
  "description": "Web interface for headscale 0.23.0",
  "homepage": "https://itsib.su",
  "license": "MIT",
  "author": {
    "email": "itsib.su@gmail.com",
    "name": "Sergey Kraynow"
  },
  "private": false,
  "type": "module",
  "browser": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsib/headscale-web.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "build": "tsc -b && NODE_ENV=production vite build --mode production",
    "build:dev": "tsc -b && NODE_ENV=development vite build --mode development",
    "build:icons": "fantasticon --config icon-font/config.cjs --normalize --round=1024 --debug",
    "lint": "eslint .",
    "test": "vitest",
    "preview": "vite preview",
    "preversion": "npm run lint && npm run test",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@codemirror/commands": "^6.7.0",
    "@codemirror/language": "^6.10.3",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.34.1",
    "@lezer/highlight": "^1.2.1",
    "@lezer/lr": "^1.4.2",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-persist-client": "^5.56.2",
    "i18next": "^23.15.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.2",
    "react-just-ui": "^0.5.11",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-visitor-keys": "^3.4.3",
    "fantasticon": "^3.0.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.1",
    "vite-plugin-compression2": "^1.3.0",
    "vitest": "^2.1.2"
  },
  "overrides": {
    "eslint-visitor-keys": {
      ".": "^3.4.3"
    },
    "@eslint-community/eslint-utils": {
      "eslint-visitor-keys": "^3.4.3"
    },
    "@typescript-eslint/visitor-keys": {
      "eslint-visitor-keys": "^3.4.3"
    }
  }
}
